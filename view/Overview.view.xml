<core:View controllerName="sap.ui.utegation.controller.Preference"
  xmlns:core="sap.ui.core" 
  xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns="sap.m"
	xmlns:l="sap.ui.layout">

	<List headerText="{i18n>OverviewText}" id="list" items="{/gasData}" noDataText="{i18n>noDataText}">

		
		<items>
			<ObjectListItem 
			  title="{Title}"
				type="Active"
				press="onListItemPress"
			>
				<firstStatus>
					<ObjectStatus
						text="{OnorOff}"
						state="{
							path: 'OnorOff',
							formatter: 'sap.ui.utegation.Formatter.Formatter.status'
						}" />
				</firstStatus>
				<attributes>
					<ObjectAttribute text="{Description}"/>
				</attributes>
			</ObjectListItem>
		</items>
	</List>
	<!--<Button press="onPressAction" text="{i18n>loadFirstItems}"></Button>-->
</core:View>