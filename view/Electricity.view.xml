<mvc:View 
    controllerName="sap.ui.utegation.controller.Preference"
    xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
    <Label text="Paperless" class="paperLess"/>
    <Switch
        state="true"
        change="onElectricityPaperLessSwitchChange"
        class="sapUiSmallMarginBottom"/>
    <List headerText=" " id="Electricity" items="{/waterData}">
  		<InputListItem label="{Title}">
  			 <Switch 
      		state="{OnorOff}"
      		change="onPreferenceSwitchChange"/>
  		</InputListItem>
  	</List>
</mvc:View>