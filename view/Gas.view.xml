<core:View controllerName="sap.ui.utegation.controller.Gas" 
  xmlns:core="sap.ui.core" 
  xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns="sap.m"
	xmlns:l="sap.ui.layout">
	<!--<Switch id="showHideHeaderSwitch"-->
	<!--	state="true"-->
	<!--	class="sapUiSmallMarginBottom"-->
	<!--	change="onHeaderSwitchChange">-->
	<!--</Switch>-->
	<List headerText="{i18n>headerText}" id="list" items="{/data}" noDataText="{i18n>noDataText}">
	  <!--<InputListItem label="WLAN">-->
			<!--  <Switch state="true" />-->
		 <!-- </InputListItem>-->
		
		<items>
			<ObjectListItem 
			  title="{Title}"
				type="Active"
				press="onListItemPress"
				number="{
					formatOptions: {showMeasure: false}
				}"
			>
				<firstStatus>
					<ObjectStatus
						text="{PaperLess}"
						state="{
							path: 'PaperLess',
							formatter: 'sap.ui.utegation.Formatter.Formatter.status'
						}" />
				</firstStatus>
				<attributes>
					<ObjectAttribute text="{Description}"/>
				</attributes>
			</ObjectListItem>
		</items>
	</List>
	<Button press="onPressAction" text="{i18n>loadFirstItems}"></Button>
</core:View>